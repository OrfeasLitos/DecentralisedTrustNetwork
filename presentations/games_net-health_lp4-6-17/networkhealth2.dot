graph G {
  bgcolor=transparent
  fontcolor=white
  fontname="montserrat"
  node [fontname="montserrat",color=white,fontcolor=white]
  edge [fontname="montserrat",color=white,fontcolor=white]
  rankdir=LR
  labelloc="b"
  label="Centrality:\ 0\n Connectedness:\ 0"
  {A B C rank=min}
  A -- H [style=invisible]
  B -- H [style=invisible]
  C -- H [style=invisible]
  H -- D [style=invisible]
  H -- E [style=invisible]
  H -- F [style=invisible]
  {D E F rank=max}
}
